import java.util.Scanner;
public class Welcome
{
    public static void main(String[] args){

        int a = 40;
        int b = 60;
        int c = 45;
         int d = (a+b+c)/2;
        System.out.println(" "+d);
        afterA();

    }
    static void afterA(){
        Scanner in = new Scanner(System.in);
        int a = in.nextInt();
        int b = in.nextInt();
        int c = in.nextInt();
        int d = (a+b+c)/2;
        System.out.println(d);

}
}
